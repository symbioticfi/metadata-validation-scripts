    "image-js": "^1.0.0",
    "json-source-map": "^0.6.1",
    "viem": "^2.37.5"
  },
  "devDependencies": {
    "@symbiotic/eslint-config": "0.1.9",
    "@symbiotic/prettier-config": "^0.0.4",
    "@github/local-action": "^6.0.0",
    "@types/node": "^24.3.1",
    "@vercel/ncc": "^0.38.3",
    "eslint": "9.35.0",
    "prettier": "3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
