{
  "name": "symbiotic-validator",
  "description": "Symbiotic Validator",
  "version": "0.0.0",
  "author": "Symbiotic",
  "type": "module",
  "private": true,
  "homepage": "https://github.com/symbioticfi/metadata-validation-scripts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/symbioticfi/metadata-validation-scripts.git"
