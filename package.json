  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@github/local-action": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vercel/ncc": "^0.38.3",
