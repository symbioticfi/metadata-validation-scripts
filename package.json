    "prettier": "^3.4.2",
    "prettier-eslint": "^16.3.0",
    "rollup": "^4.30.1",
    "ts-jest": "^29.2.5",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
